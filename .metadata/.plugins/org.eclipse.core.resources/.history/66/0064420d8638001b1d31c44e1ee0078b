package br.com.bankofoz.beans;

public class Corrente extends Cliente{

	private float limite;
	private float taxa;
	
	
	
	@Override
	public String toString() {
		return "Corrente [limite=" + limite + ", taxa=" + taxa + toString();
	}
	public Corrente() {
		super();
	}
	public Corrente(int cliente, String nome, String email, boolean especial, float limite, float taxa) {
		super(cliente, nome, email, especial);
		this.limite = limite;
		this.taxa = taxa;
	}
	public float getLimite() {
		return limite;
	}
	public void setLimite(float limite) {
		this.limite = limite;
	}
	public float getTaxa() {
		return taxa;
	}
	public void setTaxa(float taxa) {
		this.taxa = taxa;
	}
	
	
	
}
